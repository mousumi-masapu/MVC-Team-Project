<!DOCTYPE html>

<html class="no-js" lang="zxx">
<head>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link rel="icon" href="logo2.png">
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="manifest" href="site.webmanifest">
    <link rel="icon" href="../Home/Template/assets/img/logo/logo1.png">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <!-- CSS here -->
    <link rel="stylesheet" href="../Home/Template/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="../Home/Template/assets/css/owl.carousel.min.css">
    <link rel="stylesheet" href="../Home/Template/assets/css/flaticon.css">
    <link rel="stylesheet" href="../Home/Template/assets/css/slicknav.css">
    <link rel="stylesheet" href="../Home/Template/assets/css/animate.min.css">
    <link rel="stylesheet" href="../Home/Template/assets/css/magnific-popup.css">
    <link rel="stylesheet" href="../Home/Template/assets/css/fontawesome-all.min.css">
    <link rel="stylesheet" href="../Home/Template/assets/css/themify-icons.css">
    <link rel="stylesheet" href="../Home/Template/assets/css/slick.css">
    <link rel="stylesheet" href="../Home/Template/assets/css/nice-select.css">
    <link rel="stylesheet" href="../Home/Template/assets/css/style.css">
</head>
<body>

    <!-- Preloader Start -->
    <div id="preloader-active">
        <div class="preloader d-flex align-items-center justify-content-center">
            <div class="preloader-inner position-relative">
                <div class="preloader-circle"></div>
                <div class="preloader-img pere-text">
                    <img src="../Home/Template/assets/img/logo/logo1.png" alt="">
                </div>
            </div>
        </div>
    </div>
    <!-- Preloader Start -->

    <header>
        <!-- Header Start -->
        <div class="header-area">
            <div class="main-header ">

                <div class="header-bottom  header-sticky">
                    <div class="container-fluid">
                        <div class="row align-items-center">
                            <!-- Logo -->
                            <div class="col-xl-1 col-lg-1 col-md-1 col-sm-3">
                                <div class="logo">
                                    <a href="index.html"><img src="../Home/Template/assets/img/logo/logo1.png" alt=""></a>
                                </div>
                            </div>
                            <div class="col-xl-6 col-lg-8 col-md-7 col-sm-5">
                                <!-- Main-menu -->
                                <div class="main-menu f-right d-none d-lg-block">
                                    <nav>
                                        <ul id="navigation">
                                            <li>@Html.ActionLink("Products", "Index", "Products")</li>
                                            <li>@Html.ActionLink("About", "About", "Home")</li>
                                            <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                                            @if (User.IsInRole("Admin"))
                                            {
                                                <li>@Html.ActionLink("Product Admin", "Index", "ProductAdmin")</li>
                                                <li>@Html.ActionLink("Category Admin", "Index", "CategoryAdmin")</li>
                                                <li>@Html.ActionLink("Roles", "Index", "Role")</li>
                                            }
                                        </ul>
                                        @Html.Partial("_LoginPartial")
                                        @{ Html.RenderAction("BasketSummary", "Basket"); }
                                    </nav>
                                </div>
                            </div>
                            <div class="col-xl-5 col-lg-3 col-md-3 col-sm-3 fix-card">
                                <ul class="header-right f-right d-none d-lg-block d-flex justify-content-between">
                                    <li class="d-none d-xl-block">
                                        <div class="form-box f-right ">
                                            <input type="text" name="Search" placeholder="Search products">
                                            <div class="search-icon">
                                                <i class="fas fa-search special-tag"></i>
                                            </div>
                                        </div>
                                    </li>
                                    <li class=" d-none d-xl-block">
                                        <div class="favorit-items">
                                            <i class="far fa-heart"></i>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="shopping-card">
                                            <a href="cart.html"><i class="fas fa-shopping-cart"></i></a>
                                        </div>
                                    </li>
                                    <li class="d-none d-lg-block"> <a href="#" class="btn header-btn">Sign in</a></li>
                                </ul>
                            </div>
                            <!-- Mobile Menu -->
                            <div class="col-12">
                                <div class="mobile_menu d-block d-lg-none"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Header End -->
    </header>


    <div class="container body-content">
        @RenderBody()
    </div>

    <footer>

        <!-- Footer Start-->
        <div class="footer-area footer-padding fotcolor">

            <div class="container">

                <div class="row d-flex justify-content-between">
                    <div class="col-xl-3 col-lg-3 col-md-5 col-sm-6">
                        <div class="single-footer-caption mb-50">
                            <div class="single-footer-caption mb-30">
                                <!-- logo -->
                                <div class="footer-logo">
                                    <a href="index.html"><img src="../Home/Template/assets/img/logo/logo1.png" alt=""></a>
                                </div>
                                <div class="footer-tittle">
                                    <div class="footer-pera">
                                        <p>H&K is a Swedish multinational group that designs and sells ready-to-assemble household, kitchen appliances and home accessories.</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-2 col-lg-3 col-md-3 col-sm-5">
                        <div class="single-footer-caption mb-50">
                            <div class="footer-tittle">
                                <h4>Social Media</h4>
                                <div class="footer-social">
                                    <a href="#"><i class="fab fa-twitter"></i></a>
                                    <a href="#"><i class="fab fa-facebook-f"></i></a>
                                    <a href="#"><i class="fab fa-behance"></i></a>
                                    <a href="#"><i class="fas fa-globe"></i></a>
                                </div>

                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-lg-3 col-md-4 col-sm-7">
                        <div class="single-footer-caption mb-50">
                            <div class="footer-tittle">
                                <h4>Telephone Number</h4>
                                <p>08 566 107 00</p>

                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-lg-3 col-md-5 col-sm-7">
                        <div class="single-footer-caption mb-50">
                            <div class="footer-tittle">
                                <h4>Address</h4>
                                <p>Klarabergsviadukten 70, 111 64 Stockholm</p>

                            </div>
                        </div>
                    </div>
                </div>
                <!-- Footer bottom -->

                <div class="row">
                    <div class="col-xl-7 col-lg-7 col-md-7">
                        <div class="footer-copy-right">
                            <p>
                                Copyright &copy;
                                <script>document.write(new Date().getFullYear());</script> All rights reserved by H&K Household AB <i class="ti-heart" aria-hidden="true"></i>
                            </p>
                        </div>
                    </div>
                    <div class="col-xl-5 col-lg-5 col-md-5">
                        <div class="footer-copy-right f-right">

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Footer End-->

    </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <!-- JS here -->
    <!-- All JS Custom Plugins Link Here here -->
    <script src="../Home/Template/assets/js/vendor/modernizr-3.5.0.min.js"></script>
    <!-- Jquery, Popper, Bootstrap -->
    <script src="../Home/Template/assets/js/vendor/jquery-1.12.4.min.js"></script>
    <script src="../Home/Template/assets/js/popper.min.js"></script>
    <script src="../Home/Template/assets/js/bootstrap.min.js"></script>
    <!-- Jquery Mobile Menu -->
    <script src="../Home/Template/assets/js/jquery.slicknav.min.js"></script>

    <!-- Jquery Slick , Owl-Carousel Plugins -->
    <script src="../Home/Template/assets/js/owl.carousel.min.js"></script>
    <script src="../Home/Template/assets/js/slick.min.js"></script>

    <!-- One Page, Animated-HeadLin -->
    <script src="../Home/Template/assets/js/wow.min.js"></script>
    <script src="../Home/Template/assets/js/animated.headline.js"></script>
    <script src="../Home/Template/assets/js/jquery.magnific-popup.js"></script>

    <!-- Scrollup, nice-select, sticky -->
    <script src="../Home/Template/assets/js/jquery.scrollUp.min.js"></script>
    <script src="../Home/Template/assets/js/jquery.nice-select.min.js"></script>
    <script src="../Home/Template/assets/js/jquery.sticky.js"></script>

    <!-- contact js -->
    <script src="../Home/Template/assets/js/contact.js"></script>
    <script src="../Home/Template/assets/js/jquery.form.js"></script>
    <script src="../Home/Template/assets/js/jquery.validate.min.js"></script>
    <script src="../Home/Template/assets/js/mail-script.js"></script>
    <script src="../Home/Template/assets/js/jquery.ajaxchimp.min.js"></script>

    <!-- Jquery Plugins, main Jquery -->
    <script src="../Home/Template/assets/js/plugins.js"></script>
    <script src="../Home/Template/assets/js/main.js"></script>


</body>
</html>
